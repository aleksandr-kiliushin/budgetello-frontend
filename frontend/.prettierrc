{
  "arrowParens": "always",
  "bracketSpacing": true,
  "editor.formatOnSave": true,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 100,
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "importOrder": ["^@core/(.*)$", "^#", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
