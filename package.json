{
  "author": "aleksandr-kiliushin",
  "dependencies": {
    "@apollo/client": "3.7.15",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@hookform/resolvers": "3.1.1",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.13.5",
    "date-fns": "2.30.0",
    "graphql": "16.6.0",
    "history": "5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.44.3",
    "react-router-dom": "6.13.0",
    "react-use": "17.4.0",
    "recharts": "2.7.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@babel/preset-env": "7.22.5",
    "@babel/preset-react": "7.22.5",
    "@babel/preset-typescript": "7.22.5",
    "@emotion/babel-preset-css-prop": "11.11.0",
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.0",
    "@graphql-codegen/near-operation-file-preset": "2.5.0",
    "@graphql-codegen/typescript": "4.0.0",
    "@graphql-codegen/typescript-operations": "4.0.0",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/react": "18.2.12",
    "@types/react-dom": "18.2.5",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "babel-loader": "9.1.2",
    "copy-webpack-plugin": "11.0.0",
    "cypress": "12.14.0",
    "eslint": "8.43.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "webpack": "5.87.0",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1",
    "whatwg-fetch": "3.6.2"
  },
  "engines": {
    "node": "18.13.0",
    "npm": "8.19.3"
  },
  "license": "ISC",
  "name": "personal-app-frontend",
  "scripts": {
    "build": "rm -rf dist && MODE=production webpack",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "generate-webpack-bundle-report": "npm run build && webpack --profile --json > ./dist/webpack-bundle-report.json; npx webpack-bundle-analyzer ./dist/webpack-bundle-report.json",
    "graphql-codegen:dev": "MODE=development graphql-codegen --config ./codegen.ts",
    "graphql-codegen:dev:watch": "MODE=development graphql-codegen --config ./codegen.ts --watch",
    "graphql-codegen:prod": "MODE=production graphql-codegen --config ./codegen.ts",
    "cypress": "MODE=development cypress",
    "dev": "MODE=development npx webpack serve",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "prettier": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "start": "MODE=production npx webpack serve",
    "test:component": "MODE=development cypress run --component --browser chrome",
    "test:e2e": "cypress run --e2e --browser chrome"
  },
  "version": "1.0.0"
}
