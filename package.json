{
  "author": "aleksandr-kiliushin",
  "dependencies": {
    "@apollo/client": "3.7.4",
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@hookform/resolvers": "2.9.10",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.5",
    "date-fns": "2.29.3",
    "graphql": "16.6.0",
    "history": "5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.42.1",
    "react-router-dom": "6.7.0",
    "react-use": "17.4.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@emotion/babel-preset-css-prop": "11.10.0",
    "@graphql-codegen/cli": "2.16.4",
    "@graphql-codegen/client-preset": "1.2.6",
    "@graphql-codegen/near-operation-file-preset": "2.5.0",
    "@graphql-codegen/typescript": "2.8.7",
    "@graphql-codegen/typescript-operations": "2.5.12",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@trivago/prettier-plugin-sort-imports": "4.0.0",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "babel-loader": "9.1.2",
    "cypress": "12.3.0",
    "eslint": "8.32.0",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "ts-node": "10.9.1",
    "typescript": "4.9.4",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1",
    "whatwg-fetch": "3.6.2"
  },
  "engines": {
    "node": "18.13.0",
    "npm": "8.19.3"
  },
  "license": "ISC",
  "name": "personal-app-frontend",
  "scripts": {
    "build": "rm -rf dist && MODE=production webpack",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "generate-webpack-bundle-report": "npm run build && webpack --profile --json > ./dist/webpack-bundle-report.json; npx webpack-bundle-analyzer ./dist/webpack-bundle-report.json",
    "graphql-codegen": "graphql-codegen --config ./codegen.ts",
    "graphql-codegen:watch": "graphql-codegen --config ./codegen.ts --watch",
    "cypress": "MODE=development cypress",
    "dev": "MODE=development npx webpack serve",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "prettier": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "start": "MODE=production npx webpack serve",
    "test:e2e": "cypress run --e2e --browser chrome",
    "test:component": "MODE=development cypress run --component --browser chrome"
  },
  "version": "1.0.0"
}
