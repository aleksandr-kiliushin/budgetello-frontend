    "react-router-dom": "6.10.0",
    "react-use": "17.4.0",
    "recharts": "2.5.0",
    "yup": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/preset-env": "7.21.4",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.21.4",
    "@emotion/babel-preset-css-prop": "11.10.0",
    "@graphql-codegen/cli": "3.3.1",
    "@graphql-codegen/client-preset": "3.0.1",
    "@graphql-codegen/near-operation-file-preset": "2.5.0",
    "@graphql-codegen/typescript": "3.0.4",
    "@graphql-codegen/typescript-operations": "3.0.4",
    "@graphql-codegen/typescript-react-apollo": "3.3.7",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/react": "18.0.38",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.59.1",
    "@typescript-eslint/parser": "5.59.1",
    "babel-loader": "9.1.2",
    "copy-webpack-plugin": "11.0.0",
    "cypress": "12.10.0",
    "eslint": "8.39.0",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.1",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "webpack": "5.80.0",
    "webpack-cli": "5.0.2",
    "webpack-dev-server": "4.13.3",
    "whatwg-fetch": "3.6.2"
  },
  "engines": {
    "node": "18.13.0",
    "npm": "8.19.3"
  },
  "license": "ISC",
  "name": "personal-app-frontend",
  "scripts": {
    "build": "rm -rf dist && MODE=production webpack",
    "check-types": "tsc --noEmit",
    "check-types:watch": "tsc --noEmit --watch",
    "generate-webpack-bundle-report": "npm run build && webpack --profile --json > ./dist/webpack-bundle-report.json; npx webpack-bundle-analyzer ./dist/webpack-bundle-report.json",
    "graphql-codegen": "graphql-codegen --config ./codegen.ts",
    "graphql-codegen:watch": "graphql-codegen --config ./codegen.ts --watch",
    "cypress": "MODE=development cypress",
    "dev": "MODE=development npx webpack serve",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "prettier": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "start": "MODE=production npx webpack serve",
    "test:component": "MODE=development cypress run --component --browser chrome",
    "test:e2e": "cypress run --e2e --browser chrome"
  },
  "version": "1.0.0"
}
